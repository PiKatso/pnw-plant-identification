$(document).ready(function() {
  // btn cones-search
  $('#cones-btn').click(function(e){
    $('#bark-search').html('<%= j render("bark_form") %>');
    $('#cones-search').hide();
  });
  // btn bark-search
  $('#bark-btn').click(function(e){
    $('#branches-search').html('<%= j render("branches_form") %>');
    $('#bark-search').hide();
  });
});
